.width-full {
  width: 750rpx;
}

.table {
  border: 0px solid darkgray;
  font-size: 26rpx;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 82rpx;
  align-items: center;
}

.td {
  width: 33.33%;
  justify-content: center;
  text-align: center;
}

.th {
  width: 33.33%;
  justify-content: center;
  background: #0081ff;
  color: #fff;
  display: flex;
  height: 82rpx;
  line-height: 82rpx;
  align-items: center;
}